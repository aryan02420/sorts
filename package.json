{
  "name": "@aryan02420/sorts",
  "version": "0.0.1",
  "description": "multiple sorting algorithms implemented using generator functions",
  "author": "aryan02420 <aryan02420@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/aryan02420/sorts#readme",
  "keywords": [
    "sorting"
  ],
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "build": "browserify --standalone sorts ./src/sorts.js -o index.js",
    "dev": "nodemon",
    "copy": "cp -f index.js ../sort-visualiser/sorts.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aryan02420/sorts.git"
  },
  "bugs": {
    "url": "https://github.com/aryan02420/sorts/issues"
  },
  "devDependencies": {
    "browserify": "^17.0.0",
    "chai": "^4.3.3",
    "mocha": "^8.3.1",
    "nodemon": "^2.0.7"
  },
  "directories": {
    "test": "test"
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "**/node_modules",
      "demo/",
      "index.js"
    ],
    "events": {
      "restart": "npm run build && npm run copy && npm run test"
    }
  },
  "dependencies": {}
}
