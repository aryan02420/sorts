{
  "name": "@aryan02420/sorts",
  "version": "0.0.1",
  "description": "multiple sorting algorithms implemented using generator functions",
  "author": "aryan02420 <aryan02420@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/aryan02420/sorts#readme",
  "keywords": [
    "sorting"
  ],
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "build": "browserify ./src/sorts.js -s sorts | terser -c -o index.js",
    "buildDev": "browserify ./src/sorts.js -s sorts -d -o index.js",
    "dev": "nodemon",
    "copy": "cp -f index.js ../sort-visualiser/sorts.js",
    "grunt": "grunt"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aryan02420/sorts.git"
  },
  "bugs": {
    "url": "https://github.com/aryan02420/sorts/issues"
  },
  "devDependencies": {
    "browserify": "^17.0.0",
    "chai": "^4.3.3",
    "grunt": "^1.3.0",
    "grunt-browserify": "^6.0.0",
    "grunt-mocha-test": "^0.13.3",
    "grunt-terser": "^2.0.0",
    "mocha": "^8.3.1",
    "nodemon": "^2.0.7",
    "terser": "^5.6.0"
  },
  "directories": {
    "test": "test"
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "**/node_modules",
      "demo/",
      "index.js"
    ],
    "events": {
      "restart": "npm run buildDev && npm run copy && npm run test"
    }
  },
  "dependencies": {}
}
